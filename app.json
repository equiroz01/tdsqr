{
  "expo": {
    "name": "TDS QR",
    "slug": "tds-qr",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "scheme": "tdsqr",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.equiroz.tdsqr"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.equiroz.tdsqr"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-screen-orientation",
      "expo-asset",
      "expo-font",
      "expo-barcode-scanner"
    ],
    "newArchEnabled": true,
    "experiments": {
      "typedRoutes": true
    }
  }
}
